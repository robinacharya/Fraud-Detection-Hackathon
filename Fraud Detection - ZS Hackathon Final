    "tags": []
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Precision: [0.00565053 0.00565115 0.0056512  ... 1.         1.         1.        ]\n",
      "Recall: [1.         1.         1.         ... 0.20815752 0.20253165 0.        ]\n",
      "Accuracy: 0.9962568247383353\n",
      "AUROC: 0.7764591238529055\n",
      "F1 Score: 0.6258935663224782\n",
      "AUPRC: 0.597916997994588\n"
     ]
